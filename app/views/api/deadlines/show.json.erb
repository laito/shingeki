
[
	<% @deadlines.each do |d| %>
	{
	"title": "<%=d.title%>",
	"body": "<%=d.body%>",
	"user_id": <%=d.user_id%>,
	"endDateTime": "<%=d.endDateTime%>",
	<%if !d.resource_id.nil? %>
	"resource": "www.usebackpack.com/resources/<%=d.resource_id%>/download",
	<%end%>
	"timestamp": "<%=d.timestamp%>"
	} <% if not d.eql? @deadlines.last %>,<% end %>
	<%end%>

]
